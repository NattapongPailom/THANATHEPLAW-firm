{
  "projects": {
    "default": "thanatheplaw-firm"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "storage": [
    {
      "bucket": "thanatheplaw-firm.appspot.com",
      "rules": "storage.rules"
    }
  ]
}
